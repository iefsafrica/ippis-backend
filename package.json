{
  "name": "ippishr-backend",
  "version": "1.0.0",
  "main": "lib/server.js",
  "private": true,
  "type": "commonjs",
"scripts": {
  "dev": "ts-node-dev --respawn --transpile-only --require ./tsconfig-paths-bootstrap.js server.ts",
  "build": "tsc",
  "start": "node dist/server.js",
  "postinstall": "prisma generate",
  "prisma:generate": "npx prisma generate"
},
  "prisma": {
    "schema": "./lib/generated/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.10.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.15.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
